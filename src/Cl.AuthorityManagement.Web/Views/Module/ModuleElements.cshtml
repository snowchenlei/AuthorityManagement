@using Cl.AuthorityManagement.Util;
@using Cl.AuthorityManagement.Model.Mvc;
@{
    PageInfo pageInfo = Resource.PageInfos["ModuleSetElement"];

    ViewBag.Title = pageInfo.Title;
    Layout = "~/Views/Shared/_LayoutCheckElement.cshtml";
}
@Html.Raw(ViewBag.Html)

<script type="text/javascript">
    var url = @Html.Raw(pageInfo.Url);
    function CallSave(firstId, secondId) {
        $.post(@Html.Raw(pageInfo.Url), { 'firstId': firstId, 'secondId': secondId }, function (data) {
            if (data.State == 1) {
                toastr.success(data.Message);
                $('#close_edit').click();
                table.myViewModel.refresh();
            } else {
                toastr.error(data.Message);
            }
        });
    }
</script>